{
  "name": "expo-rails-7x-template",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "dotenv -- ./scripts/start-expo.sh",
    "web": "dotenv -- ./scripts/start-expo.sh --web",
    "android": "dotenv -- ./scripts/start-expo.sh --android",
    "ios": "dotenv -- ./scripts/start-expo.sh --ios",
    "start-backend": "cd backend && bin/dev",
    "gen": "node scripts/generators/index.js",
    "test": "run-p lint test:jest",
    "test:jest": "jest",
    "lint": "run-p lint:eslint lint:types",
    "lint:eslint": "eslint .",
    "lint:types": "tsc --noEmit",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.3",
    "expo": "~51.0.0",
    "expo-clipboard": "~6.0.3",
    "expo-constants": "~16.0.2",
    "expo-image-picker": "~15.1.0",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.23",
    "expo-secure-store": "~13.0.2",
    "expo-status-bar": "~1.12.1",
    "nativewind": "^4.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-web": "~0.19.10",
    "tailwindcss": "^3.4.18",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@testing-library/react-native": "~12.4.0",
    "@types/jest": "~29.5.0",
    "@types/react": "~18.2.45",
    "@typescript-eslint/eslint-plugin": "~6.21.0",
    "@typescript-eslint/parser": "~6.21.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "dotenv-cli": "^7.3.0",
    "eslint": "~8.57.0",
    "eslint-config-expo": "~7.1.2",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "jest": "~29.7.0",
    "jest-expo": "~51.0.0",
    "npm-run-all": "~4.1.5",
    "react-test-renderer": "^18.2.0",
    "typescript": "~5.3.3"
  },
  "private": true,
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
